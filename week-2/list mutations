listA = [1, 4, 3, 0]
listB = ['x', 'z', 't', 'q']

# returns the function
listA.sort
# sorts the list in place
listA.sort()
# inserts the second argument in the position noted by the first argument
listA.insert(0, 100)
# removes the value at position 3, and shortens the list
listA.remove(3)
listA.append(7)
listA.extend([4, 1, 6, 3, 4])
listA.pop(4)
listA.reverse()
print(listA)
print(listA.index(1))

listB.sort()
print(listB.pop())
#print(listB.remove('a'))

print(listA+listB)
